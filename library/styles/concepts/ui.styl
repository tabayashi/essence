$ui-layout {
  outline:0;
  margin: 0;
  border: 0;
  padding:0;
}

$ui-list {
  list-style-type: none;
}

$ui-text {
  font-size: 1rem;
  font-style: normal;
  font-weight: normal;
  font-family: monospace, monospace;
  line-height: 1;
  vertical-align: baseline;
}

$ui-link {
  text-decoration: none;
  color: inherit;
}

$ui {
  outline:0;
  margin: 0;
  border: 0;
  padding:0;
  list-style-type: none;
  font-size: 1rem;
  font-style: normal;
  font-weight: normal;
  font-family: monospace, monospace;
  line-height: 1;
  vertical-align: baseline;
  text-decoration: none;
  color: inherit;
}
